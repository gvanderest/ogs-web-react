{
  "name": "ogs-web-react",
  "version": "1.0.1",
  "description": "OGS Web Framework in React",
  "main": "index.ts",
  "repository": "https://bitbucket.org/gvanderest/ogs-web-react",
  "author": "Guillaume VanderEst <guillaume@vanderest.org>",
  "license": "MIT",
  "scripts": {
    "lint": "yarn tslint -p tsconfig.json .",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.json"
      }
    },
    "mapCoverage": true,
    "coverageDirectory": "./bypass-coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  },
  "dependencies": {
    "@types/react": "15.6.1",
    "@types/react-redux": "^5.0.1",
    "@types/react-router-dom": "^4.0.7",
    "@types/react-router-redux": "^5.0.5",
    "history": "^4.6.3",
    "moment": "^2.18.1",
    "react": "15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "@types/jest": "^20.0.6",
    "jest": "^20.0.4",
    "ts-jest": "^20.0.10",
    "tslint": "^5.6.0"
  }
}
